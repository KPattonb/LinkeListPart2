#include "node.h"
#ifndef STUDENT_H
#define STUDENT_H
#include <iomanip>
#include <iostream>
using namespace std;

class Student {
 protected:
  int id;
  char* fName[100];
  char* lName[100];
  double gpa;

 public:
  int getID();
  char* getfName();
  char* getlName();
  double getGPA();
  //constructor
  Student(char*, char*, int, double);
  //destructor
  ~Student();
};

#endif
